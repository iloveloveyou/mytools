Id');\n				if (!$carId || !is_numeric($carId)) $this->json('', 'InvalidArguments');\n				$rs = $this->carlist->remove($userId, $carId);\n				$rs ? $this->json(TRUE) : $this->json('', 'FailedOperation');\n				break;\n			//获取车列表\n			case 'GET':\n			default:\n				$rs = $this->carlist->get_user_car($userId);\n				$this->json($rs);\n				break;\n		}\n	}\n\n	/**\n	 * 生成定长随机串\n	 *\n	 * @param int $len\n	 * @return string\n	 */\n	private function genRandomNumbers($len = 6)\n	{\n		$code = array();\n		for ($i = 0; $i < $len; $i++) {\n			$code[] = rand(0, 9);\n		}\n		return implode('', $code);\n	}\n\n\n}",
